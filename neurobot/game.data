function split(inputstr, sep)
    if sep == nil then
        sep = "%s"
    end
    local t={}
    local i=1
    for str in string.gmatch(inputstr, "([^"..sep.."]+)") do
        t[i] = str
        i = i + 1
    end
    return t
end

function invert(color)
    local r, g, b, a = color[1], color[2], color[3], color[4] or 1
    return {1 - r, 1 - g, 1 - b, a}
end
