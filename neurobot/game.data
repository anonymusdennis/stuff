        if z <= n then
          if isSweet(x + _x, y + _y, z + _z) then
            return robot.distanceFromSide(x + _x, y + _y, z + _z), x + _x, y + _y, z + _z
          end
          if z ~= 0 and (fmod(l, 2) ~= 0 or z < n) then
            if isSweet(x + _x, y + _y, -z + _z) then
              return robot.distanceFromSide(x + _x, y + _y, -z + _z), x + _x, y + _y, -z + _z
